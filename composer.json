{
  "name": "obsidian-moon/engine",
  "description": "An Open Source, Lightweight and 100% Modular Framework in PHP",
  "version": "2.1.0",
  "keywords": [
    "framework"
  ],
  "homepage": "https://github.com/obsidian-moon/engine",
  "license": "MIT",
  "authors": [
    {
      "name": "Alfonso Martinez",
      "email": "opensaurusrex@gmail.com",
      "role": "Lead Developer"
    }
  ],
  "support": {
    "email": "opensaurusrex@gmail.com",
    "issues": "https://github.com/obsidian-moon/engine/issues",
    "source": "https://github.com/obsidian-moon/engine"
  },
  "require": {
    "php": ">=8.1",
    "vlucas/phpdotenv": "^5.5",
    "symfony/routing": "^6.2",
    "symfony/http-foundation": "^6.2"
  },
  "require-dev": {
    "phpmd/phpmd": "^2.13",
    "roave/security-advisories": "dev-latest",
    "squizlabs/php_codesniffer": "^3.7"
  },
  "autoload": {
    "psr-4": {
      "ObsidianMoon\\Engine\\": "src/"
    }
  },
  "config": {
    "preferred-install": {
      "roave/security-advisories": "source",
      "*": "dist"
    }
  },
  "minimum-stability": "stable"
}
