{
  "name": "obsidian-moon-development/obsidian-moon-engine",
  "description": "An Open Source, Lightweight and 100% Modular Framework in PHP",
  "version": "1.8.0",
  "keywords": [
    "framework"
  ],
  "homepage": "https://github.com/obsidian-moon-development/obsidian-moon-engine",
  "license": "MIT",
  "authors": [
    {
      "name": "Alfonso Martinez",
      "email": "opensaurusrex@gmail.com",
      "role": "Lead Developer"
    }
  ],
  "support": {
    "email": "opensaurusrex@gmail.com",
    "issues": "https://github.com/obsidian-moon-development/obsidian-moon-engine/issues",
    "source": "https://github.com/obsidian-moon-development/obsidian-moon-engine"
  },
  "require": {
    "php": ">=7.4.0",
    "ext-pdo": "*",
    "monolog/monolog": "^1.25",
    "vlucas/phpdotenv": "^3.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "ObsidianMoonDevelopment\\ObsidianMoonEngine\\": "src/"
    }
  },
  "config": {
    "preferred-install": {
      "roave/security-advisories": "source",
      "*": "dist"
    }
  },
  "minimum-stability": "stable"
}
